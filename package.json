{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write '**/*.+(js|ts|tsx)'",
    "format:ci": "prettier --check '**/*.ts{,x}'",
    "format-client": "prettier --write ./generated",
    "generate-client": "openapi --input https://vsdc.azurewebsites.net/swagger/v1/swagger.json --output ./generated --useOptions --useUnionTypes --request ./api/request.ts --client axios && yarn format-client"
  },
  "dependencies": {
    "@react-spring/web": "^9.7.3",
    "@reduxjs/toolkit": "^1.9.5",
    "@tailwindcss/forms": "^0.5.6",
    "@tanstack/react-query": "^4.36.1",
    "axios": "^1.4.0",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.12",
    "formik": "^2.4.3",
    "moment": "^2.29.4",
    "next": "13.4.12",
    "react": "18.2.0",
    "react-avatar-edit": "^1.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.10.1",
    "react-otp-input": "^3.0.4",
    "react-redux": "^8.1.2",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "tailwind-merge": "^1.14.0",
    "use-debounce": "^9.0.4",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.7.2",
    "@types/node": "20.4.8",
    "@types/react": "18.2.18",
    "autoprefixer": "^10.4.14",
    "openapi-typescript-codegen": "^0.25.0",
    "postcss": "^8.4.27",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "5.1.6"
  }
}
